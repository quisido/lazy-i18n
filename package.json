{
  "name": "lazy-i18n",
  "version": "2.0.10",
  "author": "Charles Stover <lazy-i18n@charlesstover.com>",
  "description": "Lazy loads translations for React apps",
  "homepage": "https://www.npmjs.com/package/lazy-i18n",
  "license": "MIT",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "packageManager": "yarn@3.2.1",
  "repository": "github:CharlesStover/lazy-i18n",
  "type": "module",
  "types": "./types/index.d.ts",
  "bugs": {
    "email": "lazy-i18n@charlesstover.com",
    "url": "https://github.com/CharlesStover/lazy-i18n/issues"
  },
  "directories": {
    "lib": "src"
  },
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.cjs"
  },
  "files": [
    "dist",
    "types"
  ],
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/CharlesStover"
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/cjs/index.cjs",
    "module": "./dist/esm/index.js"
  },
  "scripts": {
    "eslint": "eslint",
    "jest": "jest --runInBand",
    "rollup": "rollup --config",
    "start": "rollup --config --watch",
    "test": "eslint && jest --runInBand",
    "test-watch": "jest --coverage=false --runInBand --watch",
    "yarn-upgrade": "yarn up * && yarn up @*/*",
    "yarn-vscode": "yarn dlx @yarnpkg/pnpify --sdk vscode"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.0.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/testing-library__react-hooks": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@yarnpkg/pnpify": "^2.4.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "node-sass": "^7.0.1",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.40.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.2",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
